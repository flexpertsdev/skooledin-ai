    "@nuxtjs/tailwindcss": "^7.0.0-beta.0",
    "@pinia/nuxt": "^0.11.1",
    "@tanstack/vue-query": "^5.81.5",
    "@vite-pwa/nuxt": "^1.0.4",
    "@vueuse/core": "^13.5.0",
    "@vueuse/integrations": "^13.5.0",
    "@vueuse/nuxt": "^13.5.0",
    "dexie": "^4.0.11",
    "firebase": "^11.10.0",
